{
    "require": {
        "slim/slim": "^4.3",
        "slim/twig-view": "3.0",
        "slim/psr7": "^0.6.0",
        "php-di/php-di": "^6.0",
        "slim/http": "^0.8.0",
        "selective/config": "^0.1.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    },
    "autoload-dev": {
    "psr-4": {
        "App\\Test\\": "tests"
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    }
}
}
